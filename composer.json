{
    "name": "yproximite/payum-axepta-bnp",
    "description": "Axepta gateway for Payum",
    "keywords": ["payum", "payment", "axepta"],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "payum/core": "^1.5"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "phpstan/phpstan": "^0.12.86",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "friendsofphp/php-cs-fixer": "^2.19.0",
        "phpunit/phpunit": "^9.5.4",
        "twig/twig": "^2.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Yproximite\\Payum\\Axepta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yproximite\\Payum\\Axepta\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "ansible",
            ".php_cs",
            ".travis.yml",
            "Makefile",
            "phpstan.neon",
            "phpunit.xml",
            "Vagrantfile"
        ]
    }
}
